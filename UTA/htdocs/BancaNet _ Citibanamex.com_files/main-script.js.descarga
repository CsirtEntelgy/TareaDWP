//Get the ordinal suffix for the given int value
function getOrdinalSuffix(val){
	//var ordinalSuffixes = [ 'st', 'nd', 'rd', 'th' ];
	var mod = val % 10;
	if (mod === 1 && val !== 11) {
	  return 'st';
	} else if (mod === 2 && val !== 12) {
	  return 'nd';
	} else if (mod === 3 && val !== 13) {
	  return 'rd';
	} else {
	  return 'th';
	}
}

// Get the Date Text for the given locale string value: [es|en]
// params:  date milliseconds 
//			locale [es|en]
function getDateText(locale, pDate){
	var out = "";
	var dayNames=new Array();
	var monthNames=new Array();
   
	dayNames['es']=new Array('Domingo','Lunes','Martes','Mi&eacute;rcoles','Jueves','Viernes','Sabado')
	monthNames['es']=new Array('Enero','Febrero','Marzo','Abril','Mayo','Junio','Julio','Agosto','Septiembre','Octubre','Noviembre','Diciembre')

	dayNames['en']=new Array('Sunday','Monday','Tuesday','Wednesday','Thursday','Friday','Saturday')
	monthNames['en']=new Array('January','February','March','April','May','June','July','August','September','October','November','December')
	
	if (pDate){
		var now = new Date(pDate);
	}
	else {
		var now = new Date();
	}
		
	var dayText=dayNames[locale][now.getDay()];
	var monthText=monthNames[locale][now.getMonth()];
	
	var day=now.getDate(); //day
	var year=now.getFullYear(); //year

	//hh-mm-ss
	var hours=now.getHours();
	var minutes=now.getMinutes();
	var seconds=now.getSeconds();

	if(hours<=9)
	   hours='0'+hours;

	if(minutes<=9)
	   minutes='0'+minutes;
	
	if(seconds<=9)
	   seconds='0'+seconds;

	if (locale == "es"){
		out += dayText + " " + day + " de ";
		out += monthText + " de ";
		out += year;
		out += ", " +hours+":"+minutes+":"+ seconds ;
		
	}
	if (locale == "en"){
		out += dayText + " ";
		out += monthText + " ";
		out += day + ", ";
		out += year;
		out += ", " +hours+":"+minutes+":"+ seconds;
		
	}

	return out;
	
	
}



 
