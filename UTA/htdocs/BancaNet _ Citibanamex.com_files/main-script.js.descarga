var replaceNonAlphaNumeric = new RegExp("[^a-zA-Z0-9]+$");
function blockSpecialChars(e)
{		
	var evento = e || window.event;
	var code = getCode(e);
	
	if (e.charCode == undefined) {
		//IE
		if ((code >= 35 && code <= 39) || code == 46) {
			return false;
		}
	} else {
		
		var nav = navigator.userAgent.indexOf("Safari");
		if (nav != -1){
			// Safari
			if (((e.charCode >= 35 && e.charCode <= 39) || e.charCode == 46 ) && e.keyCode == e.charCode) {
				return false;
			}
		}
		else if(bowser.msie){
			var verified = (e.which == 8 || e.which == undefined || e.which == 0) ? null : String.fromCharCode(e.which).match(replaceNonAlphaNumeric);
			if (verified) {return false;}
		}
		if (((e.charCode >= 35 && e.charCode <= 39) || e.charCode == 46 ) && e.keyCode == 0) {
			return false;
		}
	}
	
	if ( (code >= 48 && code <= 57) || (code >= 65 && code <= 90) || (code >= 97 && code <= 122) || 
			code == 8 || code == 9 || code == 35 || code == 36 || code == 37 || code == 39 || code == 46) {
		return true;
	} else {	
		return false;
	}
}

function doBlock(e) {
	var code = getCode(e);
		
	if (e.charCode == undefined) {
		//IE
		if ((code >= 35 && code <= 39) || code == 46) {
			return false;
		}
	} else {
		if (((e.charCode >= 35 && e.charCode <= 39) || e.charCode == 46) && e.keyCode == 0) {
			return false;
		}
	}
	
	if ( (code >= 48 && code <= 57) || code == 8 || code == 9 || code == 35 || code == 36 || code == 37 || code == 39 || code == 46) {
		return true;
	} else {		
		return false;
	}
}

function getCode(e) {
	var code;
	if ((e.charCode) && (e.keyCode==0))
		code = e.charCode;
	else
		code = e.keyCode;
	
	return code;
}


function validateBrowserAndAcrobat(locale){
	var flag = true;
	var useAcrobat = isPDFInstalled();
	var nameOffset,verOffset,ix;
	var nAgt = navigator.userAgent;
	var fullVersion;
	var htmlPath;
	
	if (locale == "es"){
		htmlPath = "spanish" 
	}else{
		htmlPath = "english"
	}
	
	//var ie=(document.all)? true:false;
	var ie;
	//if (ie) {
	if (bowser.msie) {
		var Browser = {
		  Version: function() {
			var version = 999; // we assume a sane browser
			if (navigator.appVersion.indexOf("MSIE") != -1)
			  // bah, IE again, lets downgrade version number
			  version = parseFloat(navigator.appVersion.split("MSIE")[1]);
			return version;
		  }
		}
		// R13 - PCR 376 - Updated version of IE browser to 11 or superior
		ie = Browser.Version() > 11;
	}

	// R13 - PCR 376 - Updated version of ff browser to 48 or superior
	var is_ff;
	if (/Firefox[\/\s](\d+\.\d+)/.test(navigator.userAgent)){
		var ffversion=new Number(RegExp.$1);
		if (ffversion>=5) {
			is_ff = ffversion>=48;
		}
	}

	var is_chrome, is_safari, is_opera;
	if ((verOffset=nAgt.indexOf("Chrome"))!=-1) {
	 fullVersion = nAgt.substring(verOffset+7);
	 if ((verOffset=nAgt.indexOf("Version"))!=-1) 
	   fullVersion = nAgt.substring(verOffset+8);
	
	 if ((ix=fullVersion.indexOf(" "))!=-1) {
		fullVersion=fullVersion.substring(0,ix);
	 }
	 // R13 - PCR 376 - Updated version of chrome browser to 52 or superior
	 is_chrome = (parseFloat(fullVersion) >= parseFloat(52.0));
	} else if ((verOffset=nAgt.indexOf("Safari"))!=-1) {
		 fullVersion = nAgt.substring(verOffset+7);
		 if ((verOffset=nAgt.indexOf("Version"))!=-1) 
		   fullVersion = nAgt.substring(verOffset+8);
		 is_safari = (parseFloat(fullVersion) >= parseFloat(5.0));
	}
	
	// PCR 376, Java is no longer needed
	//var isJAVA = deployJava.versionCheck("1.6.0+") || deployJava.versionCheck("1.4") || deployJava.versionCheck("1.5.0*"); 
	//var isJAVA = navigator.javaEnabled();
	
	if (bowser.ios) {
		return flag;
	}
	
	if (!ie && !is_safari && !is_chrome && !is_ff && !useAcrobat){
		ventanaCompatibility("/MXGCB/JPS/portal/GetHTMLContent.do?path=/staticfiles/" + htmlPath + "/Software_Compatibility.html");
		flag = false;
		return flag;

	} else {

		if (!ie && !is_safari && !is_chrome && !is_ff && !is_opera) {

				ventanaCompatibility("/MXGCB/JPS/portal/GetHTMLContent.do?path=/staticfiles/" + htmlPath + "/Software_Compatibility.html");
				flag = false;
				return flag;

		} else {
			// fix for INC0050621115 in R13
			if (!useAcrobat && useAcrobat !== undefined) {
				ventanaCompatibility("/MXGCB/JPS/portal/GetHTMLContent.do?path=/staticfiles/" + htmlPath + "/Software_Compatibility.html");
				flag = false;
				return flag;
			}
			// fix for INC0050621115 in R13
			if (!is_chrome && is_chrome !== undefined) {
				ventanaCompatibility("/MXGCB/JPS/portal/GetHTMLContent.do?path=/staticfiles/" + htmlPath + "/Software_Compatibility.html");
				flag = false;
				return flag;
			}
		}
	}
	return flag;

}

