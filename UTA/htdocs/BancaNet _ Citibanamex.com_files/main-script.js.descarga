var cantIntentos = 0;
function validarEntrada(){
	
	
	var validation = true;
	
	var userNumberInput = document.getElementById('textCliente').value;
	countNumber = userNumberInput.length;

	if (countNumber == 0){
		displayPopup("modal_login_inputError");
		validation = false;
	}else{
		var pattern = /^\d+$/;
		var validClientNumber = userNumberInput.match(pattern);
		if(validClientNumber==null){
			document.getElementById('textCliente').value = "";
			document.getElementById('textFirma').value = "";
			displayPopup("modal_login_inputError");
			validation = false;
		}
	}
	
	
	
	var accessKeyInput = document.getElementById('textFirma').value;
	
	countKey = accessKeyInput.length;
	if (countKey == 0){
		displayPopup("modal_login_inputError");
		validation = false;
	}else{
		if(countKey < 8){
			document.getElementById('textFirma').value = "";
			displayPopup("modal_login_inputError");
			validation = false;
		}
	}
	
	if(userNumberInput==accessKeyInput){
		displayPopup("modal_login_inputError");
		validation = false;
	}
	
	if(validation){
		document.getElementById('preSignonForm').action = '/MXGCB/apps/loginbnp/flow.action';
		document.forms["preSignonForm"].submit();
	}else{
		return false;
	}
}//fin de la funciÃ³n

function autocomplet(){
	var userNumberInput = document.getElementById('textCliente').value;
	countNumber = userNumberInput.length;
	sumNumber = "";
	
	if (countNumber < 6 && countNumber > 0){
			numberAdd = 6 - countNumber;
			for ( var int = 1; int <= numberAdd; int++) {
				sumNumber = sumNumber + "0";
			}
			numberUser = sumNumber + userNumberInput;
			document.getElementById('textCliente').value = numberUser;	
	}
}



