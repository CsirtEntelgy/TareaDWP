//Description: verifies that the key is only numbers, it returns a Boolean value in response
//Param: pKey
//Return: boolean  

function onlyNumbers(pKey){
	
	var onlyNumber = 0;
	var pattern = /^\d$/;
	var character;
	for(i=0; i < pKey.length; i++){
		
		character = pKey.charAt(i);
		if(!pattern.test(character)){
			onlyNumber = 1;
		}
	}
	return onlyNumber;		
}//end function


