/*######## Common fix for all double click issues in all applications ###########*/
// add the button selector which causes the double-click issue blank page
var selector = 'a.cbolui-cta.cbolui-primary-cta,button.cbolui-cta.cbolui-primary-cta, .cN-primaryNavButton > a, button.btn-primary';

    var observeDOM = (function() {
        var MutationObserver = window.MutationObserver || window.WebKitMutationObserver,
            eventListenerSupported = window.addEventListener;

        return function(obj, callback) {
            if (MutationObserver) {
                // define a new observer
                var obs = new MutationObserver(function(mutations, observer) {
                    if (mutations[0].addedNodes.length || mutations[0].removedNodes.length)
                        callback();
                });
                // have the observer observe foo for changes in children
				try{
                obs.observe(obj, {
                    childList: true,
                    subtree: true
                });
				}catch(err){}
            } else if (eventListenerSupported) {
                obj.addEventListener('DOMNodeInserted', callback, false);
                //obj.addEventListener('DOMNodeRemoved', callback, false);
            }
        };
    })();


    $.fn.bindFirst = function(name, fn) {
        var elem, handlers, i, _len;
        this.bind(name, fn);
        for (i = 0, _len = this.length; i < _len; i++) {
            elem = this[i];
            handlers = jQuery._data(elem).events[name.split('.')[0]];
            handlers.unshift(handlers.pop());
        }
    };

	var click_timer = null;
	var globalAjax = false;

	function clickTimer(element){
		click_timer = window.setTimeout(function() {
          element.removeData('clicked');
		}, 1500)//after 1.5 seconds the button is enabled again
	}

    jQuery.fn.preventMultipleClicks = function() {
		var $el = $(this);		
		if(!globalAjax){
			$(document).ajaxSend(function() {
				clearTimeout(click_timer);
				clickTimer($el);
			});
			globalAjax = true;
		}
        $(this).bindFirst('click', function(e) {
            if ($el.data('clicked')) {
				clearTimeout(click_timer);
				clickTimer($el);
                // Previously clicked, stop actions
				//console.log('double click');
                e.stopImmediatePropagation();
                e.preventDefault();
                e.stopPropagation();
                return false;
            } else {
                // Mark to ignore next click
                $el.data('clicked', true);
                // Unmark after 1 second
                clickTimer($el);
            }
        });
		
        return this;
    };

	var clicksHandler = function(){
		$(selector).each(function() {
            if ($(this).data('multiple-clicks') === undefined) {
                $(this).data('multiple-clicks', true);
                $(this).preventMultipleClicks();
            }

        });
	}
	
$(function() {

	//Click handler initialize
    clicksHandler();

    // Observe a specific DOM element if mutates then attach prevent multiple-clicks event:
	if($('#subapp-box').length != 0){//if sub-app box container exists then we observe if mutates
    observeDOM(document.getElementById('subapp-box'), function() {
        clicksHandler();
    });
	}

});

function blockSpecialCharsAndLetters(e)
{		
	var evento = e || window.event;
	var code = getCode(e);
	
	if (e.charCode == undefined) {
		//IE
		if ((code >= 35 && code <= 39) || code == 46) {
			return false;
		}
	} else {
		
		var nav = navigator.userAgent.indexOf("Safari");
		if (nav != -1){
			// Safari
			if (((e.charCode >= 35 && e.charCode <= 39) || e.charCode == 46 ) && e.keyCode == e.charCode) {
				return false;
			}
		}
		else if(bowser.msie){
			var verified = (e.which == 8 || e.which == undefined || e.which == 0) ? null : String.fromCharCode(e.which).match(replaceNonAlphaNumeric);
			if (verified) {return false;}
		}
		if (((e.charCode >= 35 && e.charCode <= 39) || e.charCode == 46 ) && e.keyCode == 0) {
			return false;
		}
	}
	
	if ( (code >= 48 && code <= 57) || code == 8 || code == 9 || code == 35 || code == 36 || code == 37 || code == 39 || code == 46) {
		return true;
	} else {	
		return false;
	}
}

