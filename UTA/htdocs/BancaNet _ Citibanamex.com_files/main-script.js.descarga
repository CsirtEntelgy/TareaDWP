function vArriba(id, btn){
	if(act_height(id)==0){
		document.getElementById(btn).className = 'conoce_btn_trianguloA';
	$blinddown(id);
		}else{
		$blindup(id);
		document.getElementById(btn).className = 'conoce_btn_trianguloB';
